{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Launch localhost",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:4200",
            "sourceMaps": true,
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Launch index.html",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": true,
            "file": "${workspaceFolder}/index.html"
        },
    ]
}
